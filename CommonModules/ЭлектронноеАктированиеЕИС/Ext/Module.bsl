Функция ИзвлечьСведенияОПоставщикахВСтаромФормате(УзелПоставщики)
	
	Поставщики = Новый Массив;
	
	Для Каждого УзелПоставщика Из ЗначениеВКоллекцию(УзелПоставщики.supplier) Цикл
		Поставщик = НовыеСведенияОПоставщике();
		Если ЕстьСвойство(УзелПоставщика, "legalEntityRF") Тогда
			ИмяУзлаДанных = "legalEntityRF";
			Поставщик.ПолноеНаименование = УзелПоставщика[ИмяУзлаДанных].fullName;
			Поставщик.Наименование = ЗначениеНеобязательногоСвойства(
				УзелПоставщика[ИмяУзлаДанных], "shortName");
		КонецЕсли;
		Поставщики.Добавить(Поставщик);
	КонецЦикла;
	
	Возврат Поставщики;
	
КонецФункции

Функция ИзвлечьСведенияОПоставщиках(ДанныеКонтракта)

	Поставщики = Новый Массив;
	
	УзелПоставщики = ЗначениеНеобязательногоСвойства(ДанныеКонтракта, "suppliersInfo");
	Если УзелПоставщики = Неопределено Тогда
		УзелПоставщики = ЗначениеНеобязательногоСвойства(ДанныеКонтракта, "suppliers");
		Если НЕ УзелПоставщики = Неопределено Тогда
			// Применяется (обязателен) для сведений о контракте,
			// первая версия которых размещена до выхода версии 11.1.
			Возврат ИзвлечьСведенияОПоставщикахВСтаромФормате(УзелПоставщики);
		КонецЕсли;
		Возврат Поставщики;
	КонецЕсли;
	
	Для Каждого УзелПоставщика Из ЗначениеВКоллекцию(УзелПоставщики.supplierInfo) Цикл
		Поставщик = НовыеСведенияОПоставщике();
		Если ЕстьСвойство(УзелПоставщика, "legalEntityRF") Тогда
			ИмяУзлаДанных = "legalEntityRF";
			УзелИнформации = УзелПоставщика[ИмяУзлаДанных].EGRULInfo;
			Поставщик.ПолноеНаименование = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"fullName");
			Поставщик.Наименование = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"shortName");
			Поставщик.ИНН = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"INN");
			Поставщик.КПП = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"KPP");
		ИначеЕсли ЕстьСвойство(УзелПоставщика, "individualPersonRFIndEntr") Тогда
			ИмяУзлаДанных = "individualPersonRFIndEntr";
			УзелИнформации = УзелПоставщика[ИмяУзлаДанных].EGRIPInfo;
			Фамилия = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"lastName");
			Имя = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"firstName");
			Отчество = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"middleName");
			Шаблон = "%1 %2 %3";
			ПолноеНаименование = СтрШаблон(Шаблон, Фамилия, Имя, Отчество);
			ПолноеНаименование = СокрЛП(ПолноеНаименование);
			Поставщик.ПолноеНаименование = ПолноеНаименование;
			Поставщик.Наименование = ПолноеНаименование;
			Поставщик.ИНН = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"INN");
		ИначеЕсли ЕстьСвойство(УзелПоставщика, "individualPersonRF") Тогда
			ИмяУзлаДанных = "individualPersonRF";
			УзелИнформации = УзелПоставщика[ИмяУзлаДанных];
			Фамилия = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"lastName");
			Имя = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"firstName");
			Отчество = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"middleName");
			Шаблон = "%1 %2 %3";
			ПолноеНаименование = СтрШаблон(Шаблон, Фамилия, Имя, Отчество);
			ПолноеНаименование = СокрЛП(ПолноеНаименование);
			Поставщик.ПолноеНаименование = ПолноеНаименование;
			Поставщик.Наименование = ПолноеНаименование;
			Поставщик.ИНН = ЗначениеНеобязательногоСвойства(УзелИнформации,
				"INN");
		КонецЕсли;
		
		Поставщик.РеквизитыСчетаПоставщика = ИзвлечьРеквизитыСчетаПоставщика(УзелПоставщика);
		Поставщики.Добавить(Поставщик);
	КонецЦикла;
	
	Возврат Поставщики;

КонецФункции

Функция ИзвлечьСведенияОЗаказчике(ДанныеКонтракта)
	
	Сведения = НовыеСведенияОЗаказчикеКонтракта();
	
	Данные = ДанныеКонтракта.customer;
	Сведения.СокращенноеНаименование = ЗначениеНеобязательногоСвойства(Данные, "shortName");
	Сведения.ДатаПостановкиНаУчет = ЗначениеНеобязательногоСвойства(Данные, "registrationDate", "Дата");
	Сведения.ИНН = СокрЛП(ЗначениеНеобязательногоСвойства(Данные, "inn"));
	Сведения.КПП = СокрЛП(ЗначениеНеобязательногоСвойства(Данные, "kpp"));
	Сведения.ОКПО = СокрЛП(ЗначениеНеобязательногоСвойства(Данные, "OKPO"));
	Сведения.ИдентификационныйКод = ЗначениеНеобязательногоСвойства(Данные, "customerCode");
	
	КодыОрганизации = Сведения.КодыОрганизации;
	КодыОрганизации.КодПоСПЗ = Данные.regNum;
	КодыОрганизации.КодПоСводномуРеестру = ЗначениеНеобязательногоСвойства(Данные, "consRegistryNum");
	КодыОрганизации.ПолноеНаименование = ЗначениеНеобязательногоСвойства(Данные, "fullName");
	
	Возврат Сведения;
	
КонецФункции

