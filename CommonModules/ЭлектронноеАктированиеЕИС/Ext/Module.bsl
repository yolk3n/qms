// Определить вид ТРУДля УПД.
// 
// Параметры:
//  ТипОбъектаЗакупки - ПеречислениеСсылка.ТипыОбъектовЗакупкиЕИС
// 
// Возвращаемое значение:
//  Произвольный, Число - Определить вид ТРУДля УПД
Функция ОпределитьВидТРУДляУПД(ТипОбъектаЗакупки) Экспорт
	
	Если НЕ ЗначениеЗаполнено(ТипОбъектаЗакупки) Тогда
		ТекстОшибки = НСтр("ru='Не заполнен тип объекта закупки госконтракта.'");
		ВызватьИсключение(ТекстОшибки);
	КонецЕсли;
		
	// Определяем вид ТРУ.
	ВидыТРУ = Новый Соответствие;
	ВидыТРУ.Вставить(Перечисления.ТипыОбъектовЗакупкиЕИС.Товар, ВидТРУТовар());
	ВидыТРУ.Вставить(Перечисления.ТипыОбъектовЗакупкиЕИС.Работа, ВидТРУРабота());
	ВидыТРУ.Вставить(Перечисления.ТипыОбъектовЗакупкиЕИС.Услуга, ВидТРУУслуга());
	ВидТРУ = ВидыТРУ[ТипОбъектаЗакупки];
	
	Если НЕ ЗначениеЗаполнено(ВидТРУ) Тогда
		Шаблон = НСтр("ru='Не удалось определить вид ТРУ для приложения ЕИС по типу объекта закупки %1.'");
		ТекстОшибки = СтрШаблон(Шаблон, ТипОбъектаЗакупки); 
		ВызватьИсключение(ТекстОшибки);
	КонецЕсли;
	
	Возврат ВидТРУ;
	
КонецФункции

// Новые сведения о месте поставки товара.
// 
// Возвращаемое значение:
//  Структура - Новые сведения о месте поставки товара:
//    * Место - Строка
//    * Идентификатор - Строка
//    * ИдентификаторУчастника - Строка
//    * Классификатор - Строка
//    * Код - Строка
//    * Наименование - Строка
//    * Адрес - Строка
//    * РайонИлиГород - Строка
//    * НаселенныйПункт - Строка
Функция НовыеСведенияОМестеПоставкиТовара() Экспорт
	
	Результат = Новый Структура;
	// Место поставки.
	// Место, Строка 2000.
	Результат.Вставить("Место", "");
	// Идентификатор места поставки.
	// Контролируется наличие значения среди идентификаторов,
	// указанных в блоках "Сведения о месте поставки товаров"
	// (ФайлУПДПрод/СведМестаПоставки/СведМестоПоставки).
	// Добавлено в схеме версии 11.3.
	// ИдМестаПоставки, Строка 32.
	Результат.Вставить("Идентификатор", "");
	// Информация для участника документооборота.
	// Используется для идентификации заказчика/грузополучателя,
	// информация о котором указывается в основном документе.
	// Контролируется обязательность заполнения при приеме и наличие 
	// в основном документе заказчика/грузополучателя с указанным идентификатором,
	// который задается в атрибуте "Информация для участника документооборота"
	// (Файл/Документ/СвСчФакт/ГрузПолуч/@ИнфДляУчаст
	// или Файл/Документ/СвСчФакт/СвПокуп/@ИнфДляУчаст).
	// Добавлено в схеме версии 11.3.
	// ИнфДляУчаст, Строка 255.
	Результат.Вставить("ИдентификаторУчастника", "");
	// Варианты:
	// 1) КЛАДР - если поставка в РФ.
	// 2) Страна в ОКСМ - если поставка не в РФ. При приеме содержимое контролируется
	// на  присутствие в справочнике "Общероссийский классификатор стран мира (ОКСМ)" (nsiOKSM).
	// 3) Место поставки по ОКТМО.
	// см. МестоПоставкиПоКЛАДР(), МестоПоставкиПоОКТМО(), МестоПоставкиПоОКСМ().
	Результат.Вставить("Классификатор");
	// ПоКЛАДР/КЛАДР/Код, Строка 20.
	// ПоКЛАДР/Страна/Код, Строка 3.
	// ПоОКТМО/Код, Строка 11.
	Результат.Вставить("Код");
	// ПоКЛАДР/КЛАДР/Наим, Строка 200.
	// ПоКЛАДР/Страна/Наим, Строка 255.
	// ПоОКТМО/Наим, Строка 1000.
	Результат.Вставить("Наименование");
	// Адрес по КЛАДР.
	// Заполняется для ПФ адресом в соответствии со справочником КЛАДР.
	// Адрес по ОКТМО.
	// Заполняется для ПФ адресом в соответствии со справочником ОКТМО.
	// ПоКЛАДР/КЛАДР/Адрес, Строка 2000.
	// ПоОКТМО/Адрес, Строка 2000.
	Результат.Вставить("Адрес");
	// КЛАДР/НеКЛАДР/РайонГород, Строка 100.
	Результат.Вставить("РайонИлиГород");
	// Населенный пункт.
	// КЛАДР/НеКЛАДР/НаселенПункт, Строка 100.
	Результат.Вставить("НаселенныйПункт");
	
	Возврат Результат;
	
КонецФункции

// Заполнить реквизиты казначейского сопровождения поставщика.
// 
// Параметры:
//  ДанныеПриложения - Структура - данные приложения для ЕИС.
//  ДеревоДанных - ДеревоЗначений - дерево данных документа.
//  ЭтоУКД - Булево - признак УКД.
Процедура ЗаполнитьРеквизитыКазначейскогоСопровожденияПоставщика(
					ДанныеПриложения, ДеревоДанных, ЭтоУКД = Ложь) Экспорт
	
	РеквизитыКазначейскогоСопровождения = НовыеРеквизитыКазначейскогоСопровождения();
	
	Если ЭтоУКД Тогда
		СтрокаДереваДанных = ДеревоДанных;
		ВидУчастника = "СведенияОПродавце";
	Иначе
		ТаблицаПродавцов = ДеревоДанных.Строки.Найти("СведенияОПродавце", "ПолныйПуть");
		СтрокаДереваДанных = ТаблицаПродавцов.Строки[0];
		ВидУчастника = "СведенияОПродавце.НомерСтроки";
	КонецЕсли;

	БанкРеквизитыВДереве = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
		СтрокаДереваДанных, ВидУчастника + ".БанковскиеРеквизиты");
	Если ЗначениеЗаполнено(БанкРеквизитыВДереве) Тогда
		// Заполнение банковских реквизитов.
		РеквизитыКазначейскогоСопровождения.НаименованиеБанка = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
			СтрокаДереваДанных,
			ВидУчастника + ".БанковскиеРеквизиты.НаименованиеБанка");
		РеквизитыКазначейскогоСопровождения.БИК =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
			СтрокаДереваДанных,
			ВидУчастника + ".БанковскиеРеквизиты.БИКБанка");
		РеквизитыКазначейскогоСопровождения.ЕдиныйКазначейскийСчет =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
			СтрокаДереваДанных,
			ВидУчастника + ".БанковскиеРеквизиты.КорреспондентскийСчетБанка");
		РеквизитыКазначейскогоСопровождения.КазначейскийСчет =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
			СтрокаДереваДанных,
			ВидУчастника + ".БанковскиеРеквизиты.НомерСчета");
	КонецЕсли;
	
	ДанныеПриложения.РеквизитыКазначейскогоСопровождения = РеквизитыКазначейскогоСопровождения;
	
	// Заполнение кода СВР поставщика.
	Организация = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ДанныеПриложения.ГосударственныйКонтракт,
		"Организация");
	НастройкиПоставщика = НастройкиОбменаЕИС(Организация);
	КодСводногоРеестра = НастройкиПоставщика.КодСводногоРеестра;
	Если ЗначениеЗаполнено(КодСводногоРеестра) Тогда
		ДанныеПриложения.КодСВРПоставщика = КодСводногоРеестра;
	КонецЕсли;
	
	// Заполнение лицевого счета.
	ЛицевойСчет = НастройкиПоставщика.ЛицевойСчетДляКазначейскогоСопровождения;
	Если ЗначениеЗаполнено(ЛицевойСчет) Тогда
		РеквизитыКазначейскогоСопровождения.ЛицевойСчет = ЛицевойСчет;
	КонецЕсли;
	
КонецПроцедуры

// Получить данные файла БСП.
// 
// Параметры:
//  ПрисоединенныйФайл - Произвольный, Неопределено - Присоединенный файл
//  ИдентификаторФормы - Неопределено - Идентификатор формы
//  ПолучатьСсылкуНаДвоичныеДанные - Булево - Получать ссылку на двоичные данные
