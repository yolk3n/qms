// Новая таблица подписантов.
// 
// Возвращаемое значение:
//  ТаблицаЗначений - Новая таблица подписантов:
//    * НомерСтроки - Число
//    * ОбластьПолномочий - Строка
//    * Статус - Строка
//    * ОснованиеПолномочий - Строка
//    * ИНН - Строка
//    * Должность - Строка
//    * Фамилия - Строка
//    * Имя - Строка
//    * Отчество - Строка
//    * ТипПодписанта - ПеречислениеСсылка.ТипыПодписантовЕИС
//    * Организация - ОпределяемыйТип.Организация - организация подписанта.
Функция НоваяТаблицаПодписантов() Экспорт

	ТаблицаПодписанты = Новый ТаблицаЗначений;
	
	ТипОрганизация = Метаданные.ОпределяемыеТипы.Организация.Тип;
	ТипПодписанта = Новый ОписаниеТипов("ПеречислениеСсылка.ТипыПодписантовЕИС");
	ТипСтрока = Новый ОписаниеТипов("Строка");
	ТипЧисло = Новый ОписаниеТипов("Число");
	
	ТаблицаПодписанты.Колонки.Добавить("НомерСтроки", ТипЧисло);
	ТаблицаПодписанты.Колонки.Добавить("ОбластьПолномочий", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("Статус", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("ОснованиеПолномочий", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("ИНН", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("Должность", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("Фамилия", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("Имя", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("Отчество", ТипСтрока);
	ТаблицаПодписанты.Колонки.Добавить("ТипПодписанта", ТипПодписанта);
	ТаблицаПодписанты.Колонки.Добавить("Организация", ТипОрганизация);
	
	Возврат ТаблицаПодписанты;
	
КонецФункции

// Данные о полномочиях подписанта заказчика.
// 
// Параметры:
//  Подписант - СправочникСсылка.ПодписантыОрганизацийЕИС - подписант.
//  ТипДокумента - Строка - тип документа, возможные значения "УПД", "УКД".
// 
// Возвращаемое значение:
//  Структура - Данные о полномочиях подписанта заказчика:
//   * КодОбластиПолномочий - Строка
//   * НаименованиеОбластиПолномочий - Строка
//   * ОснованиеПолномочий - Строка
//   * ТипДокумента - Строка
Функция ДанныеОПолномочияхПодписантаЗаказчика(Подписант, ТипДокумента) Экспорт
	
	Результат = Новый Структура;
	Результат.Вставить("КодОбластиПолномочий", "");
	Результат.Вставить("НаименованиеОбластиПолномочий", "");
	Результат.Вставить("ОснованиеПолномочий", "");
	Результат.Вставить("ТипДокумента", "");
	
	Результат.ТипДокумента = ТипДокумента;
	
	ТипыЕИС = Новый Соответствие();
	ТипыЕИС.Вставить("УПД", "UPD");
	ТипыЕИС.Вставить("УКД", "UKD");
	ТипДокументаЕИС = ТипыЕИС[ТипДокумента];
	Если ТипДокументаЕИС = Неопределено Тогда
		ОписаниеОшибки = НСтр("ru = 'Не определен тип документа ЕИС на основании строки %1.'");
		ОписаниеОшибки = СтрШаблон(ОписаниеОшибки, ТипДокумента);
		ВызватьИсключение(ОписаниеОшибки);
	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ РАЗРЕШЕННЫЕ ПЕРВЫЕ 1
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.Ссылка,
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.НомерСтроки,
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.КодОбластиПолномочий,
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.НаименованиеОбластиПолномочий,
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.ОснованиеПолномочий
	|ИЗ
	|	Справочник.ПодписантыОрганизацийЕИС.ИнформацияОПолномочиях КАК ПодписантыОрганизацийЕИСИнформацияОПолномочиях
	|ГДЕ
	|	ПодписантыОрганизацийЕИСИнформацияОПолномочиях.Ссылка = &Подписант
	|	И ПодписантыОрганизацийЕИСИнформацияОПолномочиях.ТипДокумента = &ТипДокумента";
	
	Запрос.УстановитьПараметр("Подписант", Подписант);
	Запрос.УстановитьПараметр("ТипДокумента", ТипДокументаЕИС);
	
	УстановитьПривилегированныйРежим(Истина);
	РезультатЗапроса = Запрос.Выполнить();
	УстановитьПривилегированныйРежим(Ложь);
	
	Если РезультатЗапроса.Пустой() Тогда
		Возврат Результат;
	КонецЕсли;
	
	Выборка = РезультатЗапроса.Выбрать();
	Выборка.Следующий();
	
	ЗаполнитьЗначенияСвойств(Результат, Выборка);
	
	Возврат Результат;
	
КонецФункции

// Добавить ошибку заполнения приложения.
// 
// Параметры:
//  ДанныеПриложения - Структура - Данные приложения см. НовыеДанныеПриложенияКТитулуПродавцаУПД(), НовыеДанныеПриложенияКТитулуПродавцаУКД()
//  ТекстОшибки - Строка - текст ошибки.
// 
Процедура ДобавитьОшибкуЗаполненияПриложения(ДанныеПриложения, ТекстОшибки) Экспорт
	
	ДанныеПриложения.ОшибкиЗаполнения.Добавить(ТекстОшибки);
	
КонецПроцедуры

// Текст запроса данных шапки государственного контракта.
