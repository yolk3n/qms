	Поле = НовоеПолеСтруктуры("Наименование", "",
		Тип,
		Описание,
		Ложь,
		"@Наим");
	ДобавитьПолеСтруктуры(Результат, Поле);

	Возврат Результат;
	
КонецФункции

// См. АдресныйКлассификатор.КодыАдреса
Функция КодыАдреса(Адрес) Экспорт
	
	Если ТипЗнч(Адрес) = Тип("Структура") Тогда
		ДанныеАдреса = Адрес;
	Иначе
		ЧтениеJSON = Новый ЧтениеJSON;
		ЧтениеJSON.УстановитьСтроку(Адрес);
		ДанныеАдреса = ПрочитатьJSON(ЧтениеJSON);
		ЧтениеJSON.Закрыть();
	КонецЕсли;
	
	КодыАдреса = АдресныйКлассификатор.КодыАдреса(Адрес);
	Если НЕ ЗначениеЗаполнено(КодыАдреса.КодКЛАДР) Тогда
		КодРегиона = АдресныйКлассификатор.КодРегионаПоНаименованию(ДанныеАдреса.area);
		КодРегиона = Строка(КодРегиона);
		КодыАдреса.КодКЛАДР = КодКЛАДРПроизвольногоАдреса(КодРегиона);
	КонецЕсли;
	
	КодыАдреса.КодКЛАДР = ПреобразоватьКодКладрДляЕИС(
					КодыАдреса.КодКЛАДР);
	
	Возврат КодыАдреса;
		
КонецФункции

