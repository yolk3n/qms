		КлючСервераИнформацииДляПоставщика(),
		"lkp-get-object-info",
		ТекстЗапроса);
		
	Если НЕ РезультатОтправкиЗапроса.Выполнено Тогда
		МаксимальноеЧислоПопытокПриОшибкеСоединения = 5;
		Если РезультатОтправкиЗапроса.ОшибкаСоединения
			И НомерПопыткиПолучения < МаксимальноеЧислоПопытокПриОшибкеСоединения Тогда
