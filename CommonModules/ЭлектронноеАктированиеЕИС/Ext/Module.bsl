#Область БанковскиеРеквизиты

// Заполнение банковских реквизитов поставщика. Реквизиты заполняются если не заполнен
// реквизит БанковскиеРеквизиты.ТипСчета в ДанныеПриложения.
// 
// Параметры:
//  ДанныеПриложения - Структура - данные приложения для ЕИС.
//  ДеревоДанных - ДеревоЗначений - дерево данных документа.
//  ЭтоУКД - Булево - признак УКД.
//  ТребуетсяКазначейскоеСопровождение - Булево
Процедура ЗаполнитьБанковскиеРеквизитыПоставщика(
				ДанныеПриложения, ДеревоДанных, ЭтоУКД = Ложь,
				ТребуетсяКазначейскоеСопровождение = Ложь)
	
	Если ЗначениеЗаполнено(ДанныеПриложения.БанковскиеРеквизиты)
		И ЗначениеЗаполнено(ДанныеПриложения.БанковскиеРеквизиты.ТипСчета) Тогда
		ТипСчета = ДанныеПриложения.БанковскиеРеквизиты.ТипСчета;
		Если ТипСчета = ТипСчетаЛицевойСчетВТерОрганеКазначейства()
			ИЛИ ТипСчета = ТипСчетаЛицевойСчетВФинансовомОргане() Тогда
			ЗаполнитьКодСВРПоставщика(ДанныеПриложения);
		КонецЕсли;
		Возврат;
	КонецЕсли;
	
	БанковскиеРеквизиты = НовыеБанковскиеРеквизитыДляПриложения();
	
	Если ЭтоУКД Тогда
		СтрокаДереваДанных = ДеревоДанных;
		ВидУчастника = "СведенияОПродавце";
	Иначе
		ТаблицаПродавцов = ДеревоДанных.Строки.Найти("СведенияОПродавце", "ПолныйПуть");
		СтрокаДереваДанных = ТаблицаПродавцов.Строки[0];
		ВидУчастника = "СведенияОПродавце.НомерСтроки";
	КонецЕсли;
	
	ТипСчета = ТипСчетаЛицевойСчетВФинансовомОргане();
	
	БанкРеквизитыВДереве = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
		СтрокаДереваДанных, ВидУчастника + ".БанковскиеРеквизиты");
		
	Если ЗначениеЗаполнено(БанкРеквизитыВДереве) Тогда
		Если НЕ ТребуетсяКазначейскоеСопровождение Тогда
			
			// Заполнение банковских реквизитов расчетного счета.
			ТипСчета = ТипСчетаРасчетныйСчет();
			БанковскиеРеквизиты.ТипСчета = ТипСчета;
			БанковскиеРеквизиты.НаименованиеБанка = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.НаименованиеБанка");
			БанковскиеРеквизиты.БИК =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.БИКБанка");
			БанковскиеРеквизиты.КорреспондентскийСчетБанка =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.КорреспондентскийСчетБанка");
			БанковскиеРеквизиты.РасчетныйСчет =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.НомерСчета");
				
		Иначе
			
			// Заполнение банковских реквизитов казначейского счета.
			ТипСчета = ТипСчетаЛицевойСчетВТерОрганеКазначейства();
			БанковскиеРеквизиты.ТипСчета = ТипСчета;
			БанковскиеРеквизиты.НаименованиеБанка = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.НаименованиеБанка");
			БанковскиеРеквизиты.БИК =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.БИКБанка");
			БанковскиеРеквизиты.ЕдиныйКазначейскийСчет =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.КорреспондентскийСчетБанка");
			БанковскиеРеквизиты.КазначейскийСчет =  ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаДереваДанных,
				ВидУчастника + ".БанковскиеРеквизиты.НомерСчета");
			
			Организация = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
				ДанныеПриложения.ГосударственныйКонтракт,
				"Организация");
			НастройкиПоставщика = НастройкиОбменаЕИС(Организация);
			
			// Заполнение кода СВР поставщика.
			ЗаполнитьКодСВРПоставщика(ДанныеПриложения, НастройкиПоставщика);
			
			// Заполнение лицевого счета.
			ЛицевойСчет = НастройкиПоставщика.ЛицевойСчетДляКазначейскогоСопровождения;
			Если ЗначениеЗаполнено(ЛицевойСчет) Тогда
				БанковскиеРеквизиты.ЛицевойСчетДляКазначейскогоСопровождения = ЛицевойСчет;
			КонецЕсли;
				
		КонецЕсли;
		
		// Заполнение поля НаименованиеКонтрагентаДляПлатежногоПоручения.
		Организация = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
			ДанныеПриложения.ГосударственныйКонтракт,
			"Организация");
		НаименованиеКонтрагентаДляПлатежногоПоручения =
			ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
				Организация,
				"НаименованиеПолное");
		БанковскиеРеквизиты.НаименованиеКонтрагентаДляПлатежногоПоручения =
			Прав(НаименованиеКонтрагентаДляПлатежногоПоручения, 160);
			
		УИНИлиУИП = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
			ДанныеПриложения.ГосударственныйКонтракт,
			"ИдентификаторПлатежаИлиНачисления");
		Если ЗначениеЗаполнено(УИНИлиУИП) Тогда
			БанковскиеРеквизиты.УИПИлиУИН = УИНИлиУИП;
		КонецЕсли;
		
	КонецЕсли;
	
	ДанныеПриложения.БанковскиеРеквизиты = БанковскиеРеквизиты;
	
КонецПроцедуры

Процедура ЗаполнитьКодСВРПоставщика(ДанныеПриложения, НастройкиПоставщика = Неопределено)

	Если ЗначениеЗаполнено(ДанныеПриложения.КодСВРПоставщика) Тогда
		Возврат;
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(НастройкиПоставщика) Тогда
		Организация = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
			ДанныеПриложения.ГосударственныйКонтракт,
			"Организация");
		НастройкиПоставщика = НастройкиОбменаЕИС(Организация);
	КонецЕсли;
	
	КодСводногоРеестра = НастройкиПоставщика.КодСводногоРеестра;
	Если ЗначениеЗаполнено(КодСводногоРеестра) Тогда
