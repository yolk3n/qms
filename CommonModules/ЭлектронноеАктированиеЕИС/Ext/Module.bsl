// Проверка на вхождение продавца в список грузополучателей.
// Для прохождения "РДИК_0507. В списке грузополучателей указаны дублирующиеся грузополучатели"
// требуется чтобы в списке грузополучателей не было ИНН покупателей.
// 
// Параметры:
//  ДеревоДанных - ДеревоЗначений - Дерево данных электронного документа.
Процедура ПроверитьГрузополучателей(ДеревоДанных) Экспорт

	// Если ИНН и КПП покупателя и грузополучателя совпадают, то возникает ошибка РДИК_0507.
	// При дублировании грузополучатель удаляется, а его место поставки заменяется на покупателя.
	ТаблицаПокупателей = ДеревоДанных.Строки.Найти("СведенияОПокупателе", "ПолныйПуть");
	ВидУчастника = "СведенияОПокупателе.НомерСтроки";
	Покупатели = Новый Массив;
	Для каждого СтрокаПокупателя Из ТаблицаПокупателей.Строки Цикл
		
		ТипУчастника = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаПокупателя, ВидУчастника + ".ТипУчастника");
		ИНН = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаПокупателя,
				ВидУчастника + ".ТипУчастника." + ТипУчастника + ".ИНН");
		КПП = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаПокупателя,
				ВидУчастника + ".ТипУчастника." + ТипУчастника + ".КПП",
				Ложь);
		Покупатели.Добавить(ИНН + КПП);
		
	КонецЦикла;
	
	СтрокиКУдалению = Новый Массив;
	ИдентификаторыКЗамене = Новый Массив;
	ТаблицаГрузополучателей = ДеревоДанных.Строки.Найти("СведенияОГрузополучателе", "ПолныйПуть");
	ВидУчастника = "СведенияОГрузополучателе.НомерСтроки";
	Если ЗначениеЗаполнено(ТаблицаГрузополучателей.Значение) Тогда
		Для каждого СтрокаГрузополучателя Из ТаблицаГрузополучателей.Строки Цикл
			
			ТипУчастника = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаГрузополучателя, ВидУчастника + ".ТипУчастника");
			ИНН = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаГрузополучателя,
				ВидУчастника + ".ТипУчастника." + ТипУчастника + ".ИНН");
			КПП = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
				СтрокаГрузополучателя,
				ВидУчастника + ".ТипУчастника." + ТипУчастника + ".КПП",
				Ложь);
			Если НЕ Покупатели.Найти(ИНН + КПП) = Неопределено Тогда
				СтрокиКУдалению.Добавить(СтрокаГрузополучателя);
				Идентификатор = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
					СтрокаГрузополучателя,
					ВидУчастника + ".ИнформацияДляУчастника",
					Ложь);
				ИдентификаторыКЗамене.Добавить(Идентификатор);
			КонецЕсли;
			
		КонецЦикла;
	КонецЕсли;
	
	ДанныеПриложения = ДеревоЭлектронногоДокументаБЭД.ЗначениеРеквизитаВДереве(
					ДеревоДанных, "ДанныеЭлектронногоАктированияЕИС.ДанныеПриложения");
	
	Для Каждого Строка Из СтрокиКУдалению Цикл
		// Удаляются грузополучатели, совпадающие по ИНН и КПП с покупателем.
		ТаблицаГрузополучателей.Строки.Удалить(Строка);
	КонецЦикла;
	
	Если ИдентификаторыКЗамене.Количество() > 0 Тогда
		Для каждого Место Из ДанныеПриложения.МестаПоставки Цикл
			Если ЗначениеЗаполнено(Место.ИдентификаторУчастника)
				И НЕ ИдентификаторыКЗамене.Найти(Место.ИдентификаторУчастника) = Неопределено Тогда
				// Изменяем идентификатор места поставки на идентификатор покупателя.
				Место.ИдентификаторУчастника = НачальныйИндексИнформацииУчастникаПокупателя();
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
КонецПроцедуры

