// Создать дерево XML.
// 
// Возвращаемое значение:
//  ДеревоЗначений - Создать дерево XML:
// * Имя - Строка
// * Тип - Строка
// * Значение - Строка, Число
// * ЭтоCDATA - Булево
Функция СоздатьДеревоXML() Экспорт
	
	ДеревоXML = Новый ДеревоЗначений;
	ДеревоXML.Колонки.Добавить("Имя");
	ДеревоXML.Колонки.Добавить("Тип");
	ДеревоXML.Колонки.Добавить("Значение");
	ДеревоXML.Колонки.Добавить("ЭтоCDATA");
	
	Возврат ДеревоXML;
	
КонецФункции

// Добавить узел XML.
// 
// Параметры:
//  Родитель - ДеревоЗначений, СтрокаДереваЗначений - Родитель:
//  Имя - Строка - Имя
//  ЭтоАтрибут - Булево
//  Значение - Произвольный
//  Необязательный - Булево
//  ЭтоCDATA - Булево
//  МаксимальнаяДлинаСтрокиССокращением - Неопределено, Число
//  Индекс - Неопределено, Число
//  ПоискЭлемента - Булево
// 
// Возвращаемое значение:
//  СтрокаДереваЗначений
Функция ДобавитьУзелXML(
		Родитель,
		Имя,
		ЭтоАтрибут = Ложь,
		Значение = Неопределено,
		Необязательный = Ложь,
		ЭтоCDATA = Ложь,
		МаксимальнаяДлинаСтрокиССокращением = Неопределено,
		Индекс = Неопределено,
		ПоискЭлемента = Ложь) Экспорт
	
	Если Необязательный И НЕ ЗначениеЗаполнено(Значение) Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	Если ТипЗнч(Значение) = Тип("Число")
		ИЛИ ТипЗнч(Значение) = Тип("Дата")
		ИЛИ ТипЗнч(Значение) = Тип("Булево") Тогда
			ПриведенноеЗначение = XMLСтрока(Значение);
	Иначе
		ПриведенноеЗначение = Значение;
	КонецЕсли;
	
	Если ЗначениеЗаполнено(МаксимальнаяДлинаСтрокиССокращением) Тогда
		ПриведенноеЗначение = СокрЛП(ПриведенноеЗначение);
		Если МаксимальнаяДлинаСтрокиССокращением <> -1 Тогда
			ПриведенноеЗначение = Лев(ПриведенноеЗначение, МаксимальнаяДлинаСтрокиССокращением);
		КонецЕсли;
		ПриведенноеЗначение = XMLСтрока(ПриведенноеЗначение);
	КонецЕсли;
	
	// Ищем узел по имени.
	Если ПоискЭлемента Тогда
		НовСтр = Родитель.Строки.Найти(Имя, "Имя");
		Если ЗначениеЗаполнено(НовСтр) Тогда
			// Если узел найден, то меняем его значение.
			НовСтр.Значение = ПриведенноеЗначение;
			Возврат НовСтр;
		КонецЕсли;
	КонецЕсли;
	
	// Если узел не был найден создаем его.
	Если Индекс <> Неопределено Тогда
		НовСтр = Родитель.Строки.Вставить(Индекс);
	Иначе
		НовСтр = Родитель.Строки.Добавить();
	КонецЕсли;
	
	НовСтр.Имя = Имя;
	НовСтр.Значение = ПриведенноеЗначение;
	Если ТипЗнч(ЭтоАтрибут) = Тип("Булево") Тогда
		НовСтр.Тип = ?(ЭтоАтрибут, "А", "Э");
	Иначе
		НовСтр.Тип = ЭтоАтрибут;
	КонецЕсли;
	
	ТипЗнчРодитель = ТипЗнч(Родитель);
	Если (ТипЗнчРодитель = Тип("ДеревоЗначений") И Родитель.Колонки.Найти("ЭтоCDATA") <> Неопределено)
	ИЛИ (ТипЗнчРодитель = Тип("СтрокаДереваЗначений") И Родитель.Владелец().Колонки.Найти("ЭтоCDATA") <> Неопределено) Тогда
		НовСтр.ЭтоCDATA = ЭтоCDATA;
	КонецЕсли;
	
	Возврат НовСтр;
	
КонецФункции

// Заполнить узел дерева XMLПо структуре.
// 
// Параметры:
//  СтруктураДанных - Структура
//  УзелДерева - ДеревоЗначений,
//    СтрокаДереваЗначений
Процедура ЗаполнитьУзелДереваXMLПоСтруктуре(СтруктураДанных, Знач УзелДерева) Экспорт
	
	ВерсияСхем = ВерсияСхемЕИС();
	
	// Для формирования дерева XML по структуре поля структуры должны добавляться в 
	// той же последовательности, что и в схеме xml.
	// Сначала должны добавляться атрибуты, затем вложенные структуры или массивы.
	
	МетаданныеСтруктуры = СтруктураДанных.Метаданные; 
	
	ТипМассив = Тип("Массив");
	ТипСтруктура = Тип("Структура");
	Для Каждого ДанныеПоляСтруктуры Из СтруктураДанных Цикл
		
		ИмяПоля = ДанныеПоляСтруктуры.Ключ;
		ЗначениеПоля = ДанныеПоляСтруктуры.Значение;
		ОбязательноеПоле = МетаданныеСтруктуры.Обязательность[ИмяПоля];
		
		Если ИмяПоля = "Метаданные"
			ИЛИ ЗначениеПоля = Неопределено
			ИЛИ ОбязательноеПоле = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		ПолеНетВСхеме = МетаданныеСтруктуры.Версии[ИмяПоля].Найти(ВерсияСхем) = Неопределено;
		Если ПолеНетВСхеме Тогда
			Продолжить;
		КонецЕсли;
		
		ОписаниеТипаПоля = МетаданныеСтруктуры.ТипыЗначений[ИмяПоля];
		
		Если НЕ ЗначениеЗаполнено(ЗначениеПоля) 
			И НЕ ОбязательноеПоле Тогда
			Продолжить;
		КонецЕсли;
		
		ИмяУзла = МетаданныеСтруктуры.ПутиЗаписи[ИмяПоля];
		
		Если ОписаниеТипаПоля.СодержитТип(ТипМассив) Тогда
			ЗаполнитьУзелДереваXMLПоМассиву(ЗначениеПоля, УзелДерева, ИмяУзла);
		ИначеЕсли ОписаниеТипаПоля.СодержитТип(ТипСтруктура) Тогда
			НовоеИмяУзла = МетаданныеСтруктуры.ПутиЗаписи[ИмяПоля];
			НовыйУзелДерева = ДобавитьУзелXML(УзелДерева, НовоеИмяУзла, Ложь);
			ЗаполнитьУзелДереваXMLПоСтруктуре(ЗначениеПоля, НовыйУзелДерева);
		Иначе
			ЗаписатьПолеВДерево(УзелДерева, СтруктураДанных, ИмяПоля);
		КонецЕсли; 
		
	КонецЦикла;
	
КонецПроцедуры

// Выгрузить дерево в XML.
// 
// Параметры:
//  ДеревоВыгрузки - ДеревоЗначений
//  ИмяКорневогоЭлемента - Неопределено,
//    Строка
// 
// Возвращаемое значение:
//  Строка
Функция ВыгрузитьДеревоВXML(ДеревоВыгрузки, ИмяКорневогоЭлемента = Неопределено) Экспорт
	
	ПотокXML = СоздатьНовыйПотокXML();
	ЗаписатьУзелДереваВXML(ДеревоВыгрузки, ПотокXML, ИмяКорневогоЭлемента);
	ТекстДляЗаписи = ПотокXML.Закрыть();
	Возврат ТекстДляЗаписи;
	
КонецФункции

