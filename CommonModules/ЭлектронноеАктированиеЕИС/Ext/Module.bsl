		Значение,
		Тип,
		Описание,
		Ложь,
		"СведПоставщик/БанкРекв", ,
		"");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Код СВР поставщика.
	// Код в сводном реестре участников бюджетного процесса.
	КвалификаторыСтроки = Новый КвалификаторыСтроки(8);
	Тип = Новый ОписаниеТипов("Строка", , КвалификаторыСтроки);
