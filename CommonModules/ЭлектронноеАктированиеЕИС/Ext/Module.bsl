	Значение = Неопределено;
	Конструктор = "НовыеСведенияОКорректировкеИнформацииЗаказчика";
	Поле = НовоеПолеСтруктуры("КорректировкаИнформацииЗаказчика", Значение,
		Тип,
		Описание,
		ПризнакОбязательности,
		"Корректир", ,
		Конструктор);
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	Результат.Вставить("ИнформацияОРешенииПриемочнойКомиссии"); 
	Тип = Новый ОписаниеТипов("Структура"); 
	Описание = НСтр("ru = 'Информация о решении приемочной комиссии'"); 
	Поле = НовоеПолеСтруктуры("ИнформацияОРешенииПриемочнойКомиссии", Неопределено,
		Тип,
		Описание,
		Ложь,
		"СвПриемКом", ,
		"НовыеСведенияИнформацииОРешенииПриемочнойКомиссииЗаказчика"); 
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// См. ЭлектронноеАктированиеЗаказчикаЕИС.НовыеСведенияОПрочихНачисленияхУКДЗаказчика
	Результат.Вставить("ПрочиеНачисления"); 
	Тип = Новый ОписаниеТипов("Структура"); 
	Описание = НСтр("ru = 'Прочие начисления'"); 
	Поле = НовоеПолеСтруктуры("ПрочиеНачисления", Неопределено,
		Тип,
		Описание,
		Ложь,
		"ПрочиеНачислен",,
		"НовыеСведенияОПрочихНачисленияхУКДЗаказчика"); 
	ДобавитьПолеСтруктуры(Результат, Поле);
		
	Результат.Вставить("Вложения");
	Тип = Новый ОписаниеТипов("Массив");
	Описание = НСтр("ru = 'Прикрепленные документы'");
	Поле = НовоеПолеСтруктуры("Вложения", Новый Массив,
		Тип,
		Описание,
		Ложь,
		"Вложен", ,
		"НовыеСведенияОПрикрепленныхДокументахЗаказчика");
	ДобавитьПолеСтруктуры(Результат, Поле);
		
	Возврат Результат;
	
КонецФункции

// Новые данные приложения к титулу продавца УПД.
// 
// Возвращаемое значение:
//  Структура - Новые данные приложения к титулу продавца УПД:
//   * ПорядковыйНомерРуководителяОрганизации - Число
//   * ИдентификаторИсходногоФайла - Строка
//   * СведенияОКонтракте - см. НовыеСведенияОКонтрактеДляТитулаПродавцаУПД
//   * СведенияОТоварах - см. НовыеСведенияОТоварахРаботахУслугахДляТитулаПродавцаУПД
//   * МестаПоставки - Массив из см. НовыеСведенияОМестеПоставкиТовара
//   * СведенияОбОбжалованииОтказаОПриемке - см. НовыеСведенияОбОбжалованииОтказаОПриемке
//   * СведенияОСтроительстве - см. НовыеСведенияОСтроительствеДляПриложенияУПД
Функция НовыеДанныеПриложенияКТитулуПродавцаУПД() Экспорт
	
	Результат = Новый Структура;
	ДобавитьМетаданныеСтруктуры(Результат);
	
	ДобавитьОбщиеПоляПриложенияКТитулуПродавца(Результат);
	
	// Порядковый номер подписанта (указанный в элементе ИныеСвед),
	// являющегося руководителем организации, Число(2).
	Результат.Вставить("ПорядковыйНомерРуководителяОрганизации", 0);
	// Идентификатор файла с исходным УПД, для которого сформировано текущее Исправление.
	// Заполняется только для Исправлений УПД.
	Результат.Вставить("ИдентификаторИсходногоФайла", "");
	// Сведения о контракте.
	Результат.Вставить("СведенияОКонтракте", НовыеСведенияОКонтрактеДляТитулаПродавцаУПД());
	
	// Сведения об отгруженных товарах (о выполненных работах, оказанных услугах), переданных имущественных правах.
	// СведТов, см. НовыеСведенияОТоварахРаботахУслугахДляТитулаПродавцаУПД().
	Результат.Вставить("СведенияОТоварах", НовыеСведенияОТоварахРаботахУслугахДляТитулаПродавцаУПД());
	// Сведения о местах поставки товаров.
	// СведМестаПоставки, Массив структур, см. НовыеСведенияОМестеПоставкиТовара
	Результат.Вставить("МестаПоставки", Новый Массив);

	// Сведения досудебном (судебном) обжалования отказа в приемке товаров (работ, услуг)
	// Блок может быть заполнен только в том случае, если одновременно выполняются следующие условия:
	// функция документа отлична от СЧФ;
	// в рамках этапа контракта, к которому принимается документ, существует хотя бы один документ о приемке
	// в статусе "Подписано с частичной приемкой" или "Отказано при приемке".
	// ИтогОбжОтзакПриемТов, Неопределено, Структура см. НовыеСведенияОбОбжалованииОтказаОПриемке
	Результат.Вставить("СведенияОбОбжалованииОтказаОПриемке");
	
	// Сведения, относящиеся к отраслевой специализации по строительству.
	// Блок может быть задан только в том случае, если в атрибуте "Отраслевая специализация"
	// (ФайлУПДПрод/СведТов/@ОтрСпец) задано значение "1 - Строительство", иначе игнорируется при приеме
	// СведСтройк, Неопределено, Структура см.НовыеСведенияОСтроительствеДляПриложенияУПД. 
	Результат.Вставить("СведенияОСтроительстве");
	
	// Реквизиты сформированного в ЕИС неструктурированного документа о приемке, подтверждающего передачу.
	// Принимается только в том случае, если документ содержит функцию СЧФ. 
	// В других случаях игнорируется при приеме.
	// см. НовыеРеквизитыНеструктурированногоДокументаОПриемке
	НовыеРеквизитыНеструктурированногоДокументаОПриемке();
	Результат.Вставить("РеквизитыНеструктурированногоДокументаОПриемке", Новый Массив);
	Тип = Новый ОписаниеТипов("Массив");
	Описание = НСтр("ru = 'Реквизиты сформированного в ЕИС неструктурированного документа о приемке, подтверждающего передачу'");
	Конструктор = "НовыеРеквизитыНеструктурированногоДокументаОПриемке";
	Поле = НовоеПолеСтруктуры("РеквизитыНеструктурированногоДокументаОПриемке", "",
		Тип,
		Описание,
		Ложь,
		"НеструктДокПриемПодтвОтгр", ,
		Конструктор);
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	Возврат Результат;
	
КонецФункции

// Добавить двоичные вложения приложения для ЕИС.
// 
// Параметры:
//  ДанныеПриложенияЕИС - см. НовыеДанныеПриложенияКТитулуПродавцаУПД,
//                        см. НовыеДанныеПриложенияКТитулуПродавцаУКД
//  ДанныеЭлектронногоАктированияЕИС - Структура
Процедура ДобавитьДвоичныеВложенияПриложенияДляЕИС(ДанныеПриложенияЕИС, ДанныеЭлектронногоАктированияЕИС) Экспорт

	ПриложенныеФайлыДляЭлектронногоАктированияЕИС =
		ДанныеЭлектронногоАктированияЕИС.ПриложенныеФайлыДляЭлектронногоАктированияЕИС;
	ДопустимыеРасширения = ДопустимыеРасширенияПриложенныхФайлов();
	Для Каждого Приложение Из ПриложенныеФайлыДляЭлектронногоАктированияЕИС Цикл
		Вложение = НовыеСведенияОВложенииТитулаПродавцаУПД();
		Файл = Приложение.ПрисоединенныйФайл;
		ДанныеФайла = ПолучитьДанныеФайлаБСП(Файл);
		Расширение = ДанныеФайла.Расширение;
		Если ДопустимыеРасширения.Найти(Расширение) = Неопределено Тогда
			Шаблон = НСтр("ru='Недопустимое расширение файла вложения %1.'");
			ОписаниеОшибки = СтрШаблон(Шаблон, Расширение);
			ВызватьИсключение(ОписаниеОшибки);
		КонецЕсли;
		Вложение.ВнешнийИдентификатор = Файл.УникальныйИдентификатор();
		Вложение.ИмяФайла = ДанныеФайла.ИмяФайла;
		Вложение.РасширениеФайла = ДанныеФайла.Расширение;
		Вложение.ДатаПрикрепления = МестноеВремя(ДанныеФайла.ДатаМодификацииУниверсальная);
		Вложение.КодВидаДокумента = Приложение.КодВида;
		Вложение.НаименованиеВидаДокумента = Приложение.НаименованиеВида;
		ДанныеПриложенияЕИС.Вложения.Добавить(Вложение);
	КонецЦикла;
	
КонецПроцедуры

// Новые недетализированные сведения ОТРУ.
// 
// Возвращаемое значение:
//  Структура - Новые недетализированные сведения ОТРУ:
//   * ЭтоДетализированныеСведения - Булево
//   * НомерСтрокиТаблицы - Число
//   * ОбъемВТекстовомВыражении - Строка
//   * Идентификатор - Строка
//   * ИдентификаторПозиции - Строка - идентификатор позиции длинной 32 символа
//   * ТехническийИдентификатор - Число
//   * ВнешнийТехническийИдентификатор - Строка
//   * ИсходноеНаименование - Строка
//   * ЦенаИзКонтрактаСНДС - Число
//   * КодСтраныПроисхождения - Строка
//   * НаименованиеСтраныПроисхождения - Строка
//   * КодСтраныПроизводителя - Строка
//   * НаименованиеСтраныПроизводителя - Строка
//   * ЭтоЛекарственныйПрепарат - Булево
//   * СведенияОЛекарственномПрепарате - см. НовыеСведенияОЛекарственномПрепаратеДляПриложения
//   * ОтраслеваяСпециализация - Строка
//   * СведенияОтраслевойСпециализация - Неопределено, Структура - см. НовыеСведенияОтраслевойСпециализацииСтроительствоДляУПД
//   * АктированиеПоПозицииЗавершено - Булево
//   * ОстаточнаяСтоимостьПоПозиции - Число
Функция НовыеНедетализированныеСведенияОТРУ() Экспорт
	
	Результат = Новый Структура;
	ДобавитьМетаданныеСтруктуры(Результат);
	
	Результат.Вставить("ЭтоДетализированныеСведения", Ложь);
	// Номер строки таблицы.
	// Повторяет номер строки таблицы из файла обмена счет-фактуры (информации продавца),
	// содержащей информацию о не детализированной позиции товара, работы, услуги
	Результат.Вставить("НомерСтрокиТаблицы", 0);
	// Объем работы, услуги в текстовом выражении.
	// НедеталТРУ/НеЛПСвед/ОбРабУсл, Строка 500.
	Результат.Вставить("ОбъемВТекстовомВыражении");
	// GUID товара, работы, услуги (в случае закупки лекарственных препаратов - GUID ЛП)
	// Идентификатор товара, работы, услуги из контракта,
	// на основании которого формируется документ.
	Результат.Вставить("Идентификатор");
	// GUID товара, работы, услуги, идентифицирующий позицию в составе принимаемого документа
	// электронного актирования.
	// При передаче из внешней системы значение должно генерироваться отправляющей стороной.
	// При приеме контролируется обязательность заполнения и уникальность в рамках документа
	// электронного актирования.
	// ЭАИдТРУ, Строка(32)
	ИдентификаторПозиции = НовыйИдентификатор32();
	Результат.Вставить("ИдентификаторПозиции", ИдентификаторПозиции);
	// Технический идентификатор позиции ТРУ.
	// Соответствует значению поля sid из сведений о контракте.	
	Результат.Вставить("ТехническийИдентификатор");
	// Внешний технический идентификатор позиции ТРУ.
	// Соответствует значению поля externalSid из сведений о контракте.
	Результат.Вставить("ВнешнийТехническийИдентификатор");
	// Исходное наименование ТРУ.
	Результат.Вставить("ИсходноеНаименование", "");
	// Цена за единицу с НДС (из сведений контракта)
	// Для обычного товара, не лекарства.
	// Для лекарства заполняется сюда помещается цена за потребительскую упаковку (с НДС).
	// Для лекарств не заполняется для авансовых СЧФ, в других случаях обязателен для заполнения.
	Результат.Вставить("ЦенаИзКонтрактаСНДС");
	// Страны происхождения и производства.
	Результат.Вставить("КодСтраныПроисхождения");
	Результат.Вставить("НаименованиеСтраныПроисхождения");
	Результат.Вставить("КодСтраныПроизводителя");
	Результат.Вставить("НаименованиеСтраныПроизводителя");
	// Признак того, что объект закупки является лекарственным препаратом.
	Результат.Вставить("ЭтоЛекарственныйПрепарат", Ложь);
	// Данные лекарственного препарата.
	Результат.Вставить("СведенияОЛекарственномПрепарате", НовыеСведенияОЛекарственномПрепаратеДляПриложения());
	// Отраслевая специализация.
	// Неопределено, см. ОтраслеваяСпециализацияСтроительство(),
	// см. ОтраслеваяСпециализацияЛекарственныеПрепараты()
	// см. ОтраслеваяСпециализацияПриобретениеЖилыхПомещений()
	Результат.Вставить("ОтраслеваяСпециализация");
	// Данные отраслевой специализации.
	// Неопределено или структура НовыеСведенияОтраслевойСпециализацииСтроительствоДляУПД().
	Результат.Вставить("СведенияОтраслевойСпециализация");
	
	ДобавитьОбщиеСвойстваПозицииТРУ(Результат);
	
	// Актирование по позиции завершено.
	// Наличие данного атрибута со значением "true" означает, что актирование по данной позиции
	// из сведений о контракте завершено.
	// Указание допускается только в следующих случаях:
	// - документ не является авансовой СЧФ;
	// - в связанном контракте не установлен признак "Невозможно указать сведения о количестве товара,
	//   работы, услуги" (products/quantityUndefined);
	// - в связанном контракте не заполнен блок "Цена за право заключения контракта" (priceInfo/rightToConcludeContractPriceInfo);
	// - в основном документе для позиции заполнен атрибут "Количество (объем) (графа 3 счета-фактуры)"
	//   (Файл/Документ/ТаблСчФакт/СведТов/@КолТов);
	// - одновременно не выполняются следующие условия:
	// - в связанном контракте в поле "Способ указания цены контракта" (priceInfo/priceType)
	//    установлено значение "MP" (Максимальное значение цены контракта);
	// - в связанном контракте заполнено поле "Формула указания цены контракта" (priceInfo/priceFormula);
	// - код по ОКПД2 для текущего объекта закупки содержится в перечне кодов:
	//	19.20;
	//	06.20.10.131;
	//	65.11.10.110;
	//	65.12.11.000;
	//	65.12.12.000;
	//	65.12.21.000;
	//	65.12.34.000;
	//	65.12.41.000;
	//	65.12.49.000;
	//	65.12.50.000;
	//	65.12.90.000;
	//	06.20.10.110.
	// Если передается несколько позиций, сформированных по одной позиции из сведений о контракте,
	// то данный признак может быть задан только для одной позиции.
	// В других случаях атрибут игнорируется при приеме.
	Результат.Вставить("АктированиеПоПозицииЗавершено", "");
	Тип = Новый ОписаниеТипов("Булево");
	Описание = НСтр("ru = 'Актирование по позиции завершено.'");
	Поле = НовоеПолеСтруктуры("АктированиеПоПозицииЗавершено", "",
		Тип,
		Описание,
		Ложь,
		"@АктирЗаверш");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Остаточная стоимость по позиции (разница между стоимостью из контракта и заактированной стоимостью).
	// Игнорируется при приеме, рассчитывается автоматически.
	Результат.Вставить("ОстаточнаяСтоимостьПоПозиции", 0);
	Тип = Новый ОписаниеТипов("Число", Новый КвалификаторыЧисла(20,2));
	Описание =
		НСтр("ru = 'Остаточная стоимость по позиции (разница между стоимостью из контракта и заактированной стоимостью)'");
	Поле = НовоеПолеСтруктуры("ОстаточнаяСтоимостьПоПозиции", "",
		Тип,
		Описание,
		Ложь,
		"@ОстаточнСтоим");
	ДобавитьПолеСтруктуры(Результат, Поле);

	Возврат Результат;
	
КонецФункции

// Сведения о детализированных позициях ТРУ.
// В блоке указываются сведения о родительской позиции ТРУ и его детализациях.
// 
// Возвращаемое значение:
//  Структура - Новые сведения ОТРУДля УКД:
//   * НомерСтрокиТаблицы - Число
//   * Идентификатор - Строка
//   * ИдентификаторПозиции - Строка
//   * ТехническийИдентификатор - Строка
//   * ВнешнийТехническийИдентификатор - Строка
//   * РодительскийИдентификатор - Строка
//   * РодительскийТехническийИдентификатор - Строка
//   * РодительскийВнешнийТехническийИдентификатор - Строка
//   * Код - Строка
//   * ИсходноеНаименование - Строка
//   * ОбъемВТекстовомВыраженииДоИзменения - Строка
//   * ОбъемВТекстовомВыраженииПослеИзменения - Строка
//   * НаименованиеЕдиницыИзмеренияДоИзменения - Строка
//   * НаименованиеЕдиницыИзмеренияПослеИзменения - Строка
//   * ЦенаСНДСДоИзменения - Число
//   * ЦенаСНДСПослеИзменения - Число
//   * ЭтоЛекарственныйПрепарат - Булево
//   * СведенияОЛекарственномПрепарате - см. НовыеСведенияОЛекарственномПрепаратеДляПриложения
//   * АктированиеПоПозицииЗавершено - Булево
//   * ОстаточнаяСтоимостьПоПозиции - Число
Функция НовыеСведенияОТРУДляУКД() Экспорт
	
	Результат = Новый Структура;
	ДобавитьМетаданныеСтруктуры(Результат);
	
	// Номер строки таблицы, Число(2).
	Результат.Вставить("НомерСтрокиТаблицы");
	// GUID товара, работы, услуги, идентифицирующий позицию в составе принимаемого документа
	// электронного актирования.
	// При передаче из внешней системы значение должно генерироваться отправляющей стороной.
	// При приеме контролируется обязательность заполнения и уникальность в рамках документа
	// электронного актирования.
	// ЭАИдТРУ, Строка(32)
	Результат.Вставить("ИдентификаторПозиции", "");
	Тип = Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(32, ДопустимаяДлина.Фиксированная));
	Описание = НСтр("ru = 'GUID ТРУ, идентифицирующий позицию в составе документа электронного актирования.'");
	Поле = НовоеПолеСтруктуры("ИдентификаторПозиции", НовыйИдентификатор32(),
		Тип,
		Описание,
		Ложь,
		"@ЭАИдТРУ");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Признак Товар/Работа/Услуга.
	// 
	// Игнорируется при приеме, заполняется автоматически на основании сведений,
	// указанных в УПД (титул продавца).
	// Используется для печатной формы.
	Результат.Вставить("ПризнакТоварРаботаУслуга", "");
	Тип = Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(1, ДопустимаяДлина.Фиксированная));
	Описание = НСтр("ru = 'Признак Товар/Работа/Услуга'");
	Поле = НовоеПолеСтруктуры("ПризнакТоварРаботаУслуга", 0,
		Тип,
		Описание,
		Ложь,
		"@ПрТовРаб");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// GUID товара, работы, услуги.
	Результат.Вставить("Идентификатор");
	// Технический идентификатор позиции ТРУ.
	// Соответствует значению поля sid из сведений о контракте.
	Результат.Вставить("ТехническийИдентификатор");
	// Внешний технический идентификатор позиции ТРУ.
	// Соответствует значению поля externalSid из сведений о контракте.
	Результат.Вставить("ВнешнийТехническийИдентификатор");
	
	// GUID родительской позиции ТРУ.
	// ТРУ/ИдРодТРУ, Строка 32.
	Результат.Вставить("РодительскийИдентификатор");
	// Технический идентификатор родительской позиции ТРУ.
	// Соответствует значению поля sid из сведений о контракте.
	// ТРУ/ТехИдРодТРУ, Число.
	Результат.Вставить("РодительскийТехническийИдентификатор");
	// Внешний технический идентификатор родительской позиции ТРУ.
	// Соответствует значению поля externalSid из сведений о контракте.
	// ТРУ/ВнешТехИдРодТРУ, Строка 40.
	Результат.Вставить("РодительскийВнешнийТехническийИдентификатор");
	
	// Код товара.
	// КодТов, Строка 100.
	Результат.Вставить("Код", "");
	// Исходное наименование ТРУ.
	// НаимТовИсх, Строка 1000.
	Результат.Вставить("ИсходноеНаименование");
	
	// Сведения только для не лекарственного препарата.
	// Объем работы, услуги в текстовом выражении.
	// Используется только для работ и услуг.
	// ОбРабУсл/ДоИзм, Строка 500.
	Результат.Вставить("ОбъемВТекстовомВыраженииДоИзменения", );
	// ОбРабУсл/ПослеИзм, Строка 500.
	Результат.Вставить("ОбъемВТекстовомВыраженииПослеИзменения", );
	// Наименование единицы измерения до и после изменения.
	// НаимЕдИзм/ДоИзм, Строка 255.
	Результат.Вставить("НаименованиеЕдиницыИзмеренияДоИзменения");
	// НаимЕдИзм/ПослеИзм, Строка 255.
	Результат.Вставить("НаименованиеЕдиницыИзмеренияПослеИзменения");
	// Цена за единицу с НДС до и после изменения.
	// ЦенаСНДС/ДоИзм, Число(27, 11)
	Результат.Вставить("ЦенаСНДСДоИзменения");
	// ЦенаСНДС/ПослеИзм, Число(27, 11)
	Результат.Вставить("ЦенаСНДСПослеИзменения");
	
	// Признак лекарственного препарата.
	Результат.Вставить("ЭтоЛекарственныйПрепарат", Ложь);
	// Данные лекарственного препарата.
	Результат.Вставить("СведенияОЛекарственномПрепарате", НовыеСведенияОЛекарственномПрепаратеДляПриложения(Истина));
	
	// Актирование по позиции завершено.
	// Наличие данного атрибута со значением "true" означает, что актирование по данной позиции
	// из сведений о контракте завершено.
	// Указание допускается только в следующих случаях:
	// - документ не является авансовой СЧФ;
	// - в связанном контракте не установлен признак "Невозможно указать сведения о количестве товара,
	//   работы, услуги" (products/quantityUndefined);
	// - в связанном контракте не заполнен блок "Цена за право заключения контракта" (priceInfo/rightToConcludeContractPriceInfo);
	// - в основном документе для позиции заполнен атрибут "Количество (объем) (графа 3 счета-фактуры)"
	//   (Файл/Документ/ТаблСчФакт/СведТов/@КолТов);
	// - одновременно не выполняются следующие условия:
	// - в связанном контракте в поле "Способ указания цены контракта" (priceInfo/priceType)
	//    установлено значение "MP" (Максимальное значение цены контракта);
	// - в связанном контракте заполнено поле "Формула указания цены контракта" (priceInfo/priceFormula);
	// - код по ОКПД2 для текущего объекта закупки содержится в перечне кодов:
	//	19.20;
	//	06.20.10.131;
	//	65.11.10.110;
	//	65.12.11.000;
	//	65.12.12.000;
	//	65.12.21.000;
	//	65.12.34.000;
	//	65.12.41.000;
	//	65.12.49.000;
	//	65.12.50.000;
	//	65.12.90.000;
	//	06.20.10.110.
	// Если передается несколько позиций, сформированных по одной позиции из сведений о контракте,
	// то данный признак может быть задан только для одной позиции.
	// В других случаях атрибут игнорируется при приеме.
	Результат.Вставить("АктированиеПоПозицииЗавершено", "");
	Тип = Новый ОписаниеТипов("Булево");
	Описание = НСтр("ru = 'Актирование по позиции завершено.'");
	Поле = НовоеПолеСтруктуры("АктированиеПоПозицииЗавершено", "",
		Тип,
		Описание,
		Ложь,
		"@АктирЗаверш");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Остаточная стоимость по позиции (разница между стоимостью из контракта и заактированной стоимостью).
	// Игнорируется при приеме, рассчитывается автоматически.
	Результат.Вставить("ОстаточнаяСтоимостьПоПозиции", 0);
	Тип = Новый ОписаниеТипов("Число", Новый КвалификаторыЧисла(20,2));
	Описание =
		НСтр("ru = 'Остаточная стоимость по позиции (разница между стоимостью из контракта и заактированной стоимостью)'");
	Поле = НовоеПолеСтруктуры("ОстаточнаяСтоимостьПоПозиции", "",
		Тип,
		Описание,
		Ложь,
		"@ОстаточнСтоим");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	Возврат Результат;
	
КонецФункции

