Процедура ЗаполнитьДанныеПриложенияПродавца(ДанныеПриложения, ДвоичныеДанныеПриложения, ТипПриложения)
	
	Квалификатор = Новый КвалификаторыСтроки(32);
	ТипСтрока32 = Новый ОписаниеТипов("Строка", , Квалификатор);
	Квалификатор = Новый КвалификаторыСтроки(40);
	ТипСтрока40 = Новый ОписаниеТипов("Строка", , Квалификатор);
	Квалификатор = Новый КвалификаторыСтроки(100);
	ТипСтрока100 = Новый ОписаниеТипов("Строка", , Квалификатор);
	Квалификатор = Новый КвалификаторыСтроки(500);
	ТипСтрока500 = Новый ОписаниеТипов("Строка", , Квалификатор);
	Квалификатор = Новый КвалификаторыСтроки(1000);
	ТипСтрока1000 = Новый ОписаниеТипов("Строка", , Квалификатор);
	Квалификатор = Новый КвалификаторыЧисла(6);
	ТипЧисло6 = Новый ОписаниеТипов("Число", Квалификатор);
	Квалификатор = Новый КвалификаторыЧисла(30);
	ТипЧисло30 = Новый ОписаниеТипов("Число", Квалификатор);
	
	ТаблицаНоменклатуры = Новый ТаблицаЗначений;
	ТаблицаНоменклатуры.Колонки.Добавить("Идентификатор", ТипСтрока32);
	ТаблицаНоменклатуры.Колонки.Добавить("ТехническийИдентификатор", ТипЧисло30);
	ТаблицаНоменклатуры.Колонки.Добавить("ВнешнийИдентификатор", ТипСтрока40);
	ТаблицаНоменклатуры.Колонки.Добавить("НомерСтроки", ТипЧисло6);
	ТаблицаНоменклатуры.Колонки.Добавить("Наименование", ТипСтрока1000);
	ТаблицаНоменклатуры.Колонки.Добавить("КодТовараДляЕИС", ТипСтрока100);
	ТаблицаНоменклатуры.Колонки.Добавить("ОбъемРаботыУслуги", ТипСтрока500);
	ТаблицаНоменклатуры.Колонки.Добавить("ОбъемРаботыУслугиПослеИзменения", ТипСтрока500);
	
	Кодировка = КодировкаТекстаПриложенияДляЕИС();
	ТекстПриложения = ПолучитьСтрокуИзДвоичныхДанных(ДвоичныеДанныеПриложения, Кодировка);

	ЧтениеФайла = Новый ЧтениеXML;
	ЧтениеФайла.УстановитьСтроку(ТекстПриложения);
	ДанныеФайлаПриложения = ФабрикаXDTO.ПрочитатьXML(ЧтениеФайла);
	ЧтениеФайла.Закрыть();
	
	ЭтоКорректировка = ТипПриложения = ТипПриложенияУКДПродавца();
	
	Если ЭтоКорректировка Тогда
		УзелДанныхПоТРУ = "ТРУ";
	Иначе
		УзелДанныхПоТРУ = "СведТРУ";
	КонецЕсли;
	
	СведенияОТоварах = ЗначениеВКоллекцию(ДанныеФайлаПриложения.СведТов[УзелДанныхПоТРУ]);
	
	Для Каждого СтрокаСведенийОТоварах Из СведенияОТоварах Цикл
		
		НоваяСтрока = ТаблицаНоменклатуры.Добавить();

		Если ЕстьСвойство(СтрокаСведенийОТоварах, "НедеталТРУ") ИЛИ ЭтоКорректировка Тогда

			Если ЭтоКорректировка Тогда
				ДанныеТовараБезДетализации = СтрокаСведенийОТоварах;
				НоваяСтрока.КодТовараДляЕИС = ДанныеТовараБезДетализации.КодТов;
			Иначе
				ДанныеТовараБезДетализации = СтрокаСведенийОТоварах.НедеталТРУ;
			КонецЕсли;
			
			ЗаполнитьИдентификаторыНоменклатуры(НоваяСтрока, ДанныеТовараБезДетализации);
			НоваяСтрока.Наименование = ДанныеТовараБезДетализации.НаимТовИсх;
			
			Если ЕстьСвойство(ДанныеТовараБезДетализации, "НеЛПСвед") Тогда
				
				НеЛПСвед = ДанныеТовараБезДетализации.НеЛПСвед;
				
				НоваяСтрока.НомерСтроки = НеЛПСвед.НомСтр;
				
				Если НЕ ЭтоКорректировка Тогда
					
					НоваяСтрока.ОбъемРаботыУслуги = ЗначениеНеобязательногоСвойства(
						НеЛПСвед, "ОбРабУсл");
					
				Иначе
					
					Если ЕстьСвойство(НеЛПСвед, "ОбРабУсл") Тогда
						НоваяСтрока.ОбъемРаботыУслуги = НеЛПСвед.ОбРабУсл.ДоИзм;
						НоваяСтрока.ОбъемРаботыУслугиПослеИзменения = НеЛПСвед.ОбРабУсл.ПослеИзм;
					КонецЕсли;
					
				КонецЕсли;
				
			КонецЕсли;
			
		Иначе
			
			// Сведения о детализированных ТРУ
			Если ЭтоКорректировка Тогда
				НоваяСтрока = ТаблицаНоменклатуры.Добавить();
				ДанныеТовараСДетализацией = СтрокаСведенийОТоварах;
				ЗаполнитьИдентификаторыНоменклатуры(НоваяСтрока, ДанныеТовараСДетализацией);
				НоваяСтрока.КодТовараДляЕИС = ДанныеТовараСДетализацией.КодТов;
				НоваяСтрока.НомерСтроки = ДанныеТовараСДетализацией.НомСтр;
				НоваяСтрока.Наименование = ДанныеТовараСДетализацией.НаимТовИсх;
			Иначе
				ДанныеТовараСДетализацией = СтрокаСведенийОТоварах.ДеталТРУ;
				СведенияРодителя = ДанныеТовараСДетализацией.СведРод;
				Для Каждого СведенияДетализации Из ЗначениеВКоллекцию(ДанныеТовараСДетализацией.СведДетал) Цикл
					НоваяСтрока = ТаблицаНоменклатуры.Добавить();
					Если НЕ ЭтоКорректировка Тогда
						НоваяСтрока.НомерСтроки = СведенияДетализации.НомСтр;
						НоваяСтрока.ОбъемРаботыУслуги = ЗначениеНеобязательногоСвойства(
							СведенияДетализации, "ОбРабУсл");
						ЗаполнитьИдентификаторыНоменклатуры(НоваяСтрока, СведенияРодителя);
						НоваяСтрока.Наименование = СведенияРодителя.НаимТов;
						НоваяСтрока.КодТовараДляЕИС = СведенияРодителя.КодТов;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЦикла;
	
	ДанныеПриложения.ТаблицаИдентификаторовНоменклатуры = ТаблицаНоменклатуры;
	
КонецПроцедуры

Функция ОпределитьТипПриложенияПоТипуДокумента(ТипДокумента)
	
	// Определяем тип приложения.
	ПриложениеУПД = ТипПриложенияУПДПродавца();
	ПриложениеУКД = ТипПриложенияУКДПродавца();
	ПриложениеУПДПокупателя = ТипПриложенияУПДПокупателя();
	ПриложениеУКДПокупателя = ТипПриложенияУКДПокупателя();
	
	ТипыДокументов = Новый Соответствие;
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.УПД, ПриложениеУПД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.УКД, ПриложениеУКД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.СчетФактура, ПриложениеУПД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.ТоварнаяНакладная, ПриложениеУПД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.АктВыполненныхРабот, ПриложениеУПД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.СоглашениеОбИзмененииСтоимости, ПриложениеУКД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.КорректировочныйСчетФактура, ПриложениеУКД);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.ДокументПриемкиЕИСУПД, ПриложениеУПДПокупателя);
	ТипыДокументов.Вставить(Перечисления.ТипыДокументовЭДО.ДокументПриемкиЕИСУКД, ПриложениеУКДПокупателя);
	ТипПриложения = ТипыДокументов[ТипДокумента];
	
	Возврат ТипПриложения;
	
КонецФункции

