	Поле = НовоеПолеСтруктуры("КодСВРПоставщика",
		Значение,
		Тип,
		Описание,
		ПризнакОбязательности,
		"СведЗаказчик/КодСВР");
	Результат.Вставить("КодСВРПоставщика", "");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Код СВР заказчика.
	// Игнорируется при приеме, заполняется при передаче кодом СВР из сводного реестра организаций.
	КвалификаторыСтроки = Новый КвалификаторыСтроки(8);
	Тип = Новый ОписаниеТипов("Строка", , КвалификаторыСтроки);
	Описание = НСтр("ru = 'Код СВР заказчика'");
	Значение = "";
	ПризнакОбязательности = Ложь;
	Поле = НовоеПолеСтруктуры("КодСВРЗаказчика",
		Значение,
		Тип,
		Описание,
		ПризнакОбязательности,
		"СведЗаказчик/КодСВР");
	Результат.Вставить("КодСВРЗаказчика", "");
	ДобавитьПолеСтруктуры(Результат, Поле);
	
	// Заказчик включен в список грузополучателей.
	// Атрибут может быть заполнен только в том случае, если для заказчика указано хотя бы одно место поставки.
	Результат.Вставить("ЗаказчикВключенВСписокГрузоПолучателей", "");
	Тип = Новый ОписаниеТипов("Булево");
	Описание = НСтр("ru = 'Заказчик включен в список грузополучателей.'");
	Поле = НовоеПолеСтруктуры("ЗаказчикВключенВСписокГрузоПолучателей", Ложь,
		Тип,
		Описание,
		Ложь,
		"СведЗаказчик/@ЗаказчикВключГрузПолуч");
	ДобавитьПолеСтруктуры(Результат, Поле);

	// Прикрепленные документы.
	// Вложен, Массив структур НовыеСведенияОВложенииТитулаПродавцаУПД().
	Результат.Вставить("Вложения", Новый Массив);
	
КонецПроцедуры

Функция НовыеРеквизитыКазначейскогоСопровождения()
	
	Результат = Новый Структура;
	
	ДобавитьМетаданныеСтруктуры(Результат);
	
	// Лицевой счет для казначейского сопровождения.
	// При приеме проверяется наличие лицевого счета для казначейского сопровождения в связанных
	// сведениях о контракте:
	//  - если в сведениях о контракте поле указано, то атрибут игнорируется при приеме, при
	//     передаче заполняется значением из сведений о контракте;
	//  - если в сведениях о контракте поле не указано, то выполняется поиск лицевого счета для 
	//      казначейского сопровождения в книге регистрации лицевых счетов. Если значение найдено,
	//    то атрибут игнорируется при приеме, при передаче заполняется найденным значением,
	//      иначе значение атрибута принимается из пакета,
	//    при этом контролируется обязательность заполнения.
	КвалификаторыСтроки = Новый КвалификаторыСтроки(11);
