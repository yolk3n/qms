	Описание = НСтр("ru = 'Полное наименование.'");
	Поле = НовоеПолеСтруктуры("ПолноеНаименование", "",
		Тип,
		Описание,
		Ложь,
		"@Наим", ,
		"");
	ДобавитьПолеСтруктуры(Результат, Поле);

	Возврат Результат;
	
КонецФункции

